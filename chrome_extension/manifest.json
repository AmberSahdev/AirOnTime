{
  "manifest_version": 2,

  "name": "AirOnTime",
  "description": "This extension enables you to use AirOnTime without leaving your tab",
  "version": "0.1",

  "icons": { "16" : "img/nounproject.jpeg",
             "48" : "img/nounproject.jpeg",
             "128" : "img/nounproject.jpeg"},
  "homepage_url": "http://www.aironti.me/",
        

  "browser_action": {
    "default_icon": "img/nounproject.jpeg",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"

  ]
}
